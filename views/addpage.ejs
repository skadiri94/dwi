        <title>DWI</title>
        <!-- Favicon-->
        <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
        <!-- Core theme CSS (includes Bootstrap)-->
        <link href="assets/css/styles.css" rel="stylesheet" />
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-light bg-light border-bottom">
            <div class="container-fluid">
            
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span></button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ms-auto mt-2 mt-lg-0">
                        <li class="nav-item active"><a class="nav-link" href="/">Home</a></li>
                    </ul>
                </div>
            </div>
        </nav>
        <div class="container">
   
        <form  method="post" action='/product/add' class="form-signin">
            <div class="form-group">
              <label for="title">Type</label>
              <input type="text" class="form-control" name="type" placeholder="Enter Type" required>
              <label for="stock">Title</label>
              <input type="text" class="form-control" name="title" placeholder="Enter Title" required>
              <label for="title">Condition</label>
              <input type="text" class="form-control" name="condition" placeholder="Enter Condition" required>
              <label for="stock">Stock</label>
              <input type="number" class="form-control" name="stock" placeholder="Enter stock" required step=".01">
              <label for="stock">Category</label>
              <input type="text" class="form-control" name="category" placeholder="Enter Category" required>
              <label for="stock">Price</label>
              <input type="number" class="form-control" name="price" placeholder="Enter price" required step=".01">
              <label for="title">Year</label>
              <input type="text" class="form-control" name="year" placeholder="Enter year" required>
              <label for="stock">Average</label>
              <input type="number" class="form-control" name="average" placeholder="Enter average" required step=".01">
              <label for="stock">Count</label>
              <input type="number" class="form-control" name="count" placeholder="Enter count" required step=".01">
              <label for="stock">Description</label>
              <input type="text" class="form-control" name="description" placeholder="Enter description" required>
              
              <button type="submit" class="btn btn-primary mt-2">Add Product</button>
            </div>
        </form>

    </div>
        <!-- Bootstrap core JS-->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"></script>
        <!-- Core theme JS-->
        <script src="assets/js/scripts.js"></script>
    </body>
